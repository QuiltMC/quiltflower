   public void testModifyLeak(Object obj) {
      Integer i = null;// 121

      while(obj instanceof Integer) {// 122
         i = (Integer)obj;// 123
         if (i > 4) {// 125
            i = i + 3;// 126
         }
      }

      if (i != null) {// 130
         System.out.println(i);// 131
      }

   }// 133
